{
  "name": "@arcana/ca-common",
  "version": "0.0.1",
  "description": "common utilities for CA",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js"
    },
    "./fuelcontracts": {
      "types": "./dist/types/fuelcontracts/index.d.ts",
      "require": "./dist/cjs/fuelcontracts/index.js",
      "import": "./dist/esm/fuelcontracts/index.js"
    }
  },
  "scripts": {
    "build": "run-s clean build:esm build:cjs",
    "build:esm": "tsc --project tsconfig.json",
    "build:cjs": "tsc --project tsconfig.cjs.json",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint",
    "clean": "rimraf dist/",
    "prepublishOnly": "npm run build"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.0",
    "@eslint/js": "^9.22.0",
    "@types/node": "^22.13.10",
    "@typescript-eslint/eslint-plugin": "^8.27.0",
    "@typescript-eslint/parser": "^8.27.0",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.1",
    "globals": "^16.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "ts-proto": "^2.6.1",
    "typescript": "^5.8.2"
  },
  "peerDependencies": {
    "@cosmjs/proto-signing": "^0.33.0",
    "@cosmjs/stargate": "^0.33.0",
    "axios": "^1.8.4",
    "decimal.js": "^10.4.3",
    "fuels": "^0.100.1",
    "long": "^5.3.1",
    "viem": "^2.23.13"
  },
  "dependencies": {
    "@bufbuild/protobuf": "^2.2.5",
    "@improbable-eng/grpc-web": "^0.15.0",
    "browser-headers": "^0.4.1",
    "es-toolkit": "^1.33.0",
    "tslib": "^2.8.1"
  }
}
